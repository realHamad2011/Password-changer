<Window x:Class="Password_changer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Password_changer"
        mc:Ignorable="d"
        Title="Password changer" Height="135" Width="325">
    <Grid>
        <Button Content="Change" HorizontalAlignment="Left" Margin="261,0,0,0" VerticalAlignment="Center" Click="Button_Click"/>
        <TextBox x:Name="Textboxhamad" HorizontalAlignment="Left" Margin="118,0,0,0" Text="Password" TextWrapping="Wrap" VerticalAlignment="Center" Width="120" TextChanged="Textboxhamad_TextChanged"/>
        <TextBox x:Name="textboxhamad2" Margin="10,0,212,0" Text="User name" TextWrapping="Wrap" VerticalAlignment="Center" Height="20"/>
    </Grid>
</Window>
